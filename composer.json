{
    "name": "slps970093/live100ms",
    "description": "100ms live rest api sdk",
    "keywords": [
        "slps970093",
        "100ms"
    ],
    "homepage": "https://github.com/slps970093/live100ms",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Yu-Hsien, Chou",
            "email": "slps970093@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "firebase/php-jwt": "^6.3",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/support": "^8.0|^9.0",
        "laravel/framework": "^8.0|^9.0",
        "nesbot/carbon": "^2.0.0",
        "orchestra/testbench": "^6.6|^v7.6",
        "symfony/property-access": "^5.4|^6.2",
        "symfony/serializer": "^5.4|^6.2",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Slps970093\\Live100ms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slps970093\\Live100ms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Slps970093\\Live100ms\\Live100msServiceProvider"
            ],
            "aliases": {
                "Live100ms": "Slps970093\\Live100ms\\Live100msFacade"
            }
        }
    }
}
